
void setup() {
  Serial.begin(9600);
  //pinMode(Flex_pin, INPUT);
  //pinMode(Flex_pin1, INPUT);
}

void loop() {
  //read first flex sensor
  int sensorValue = analogRead(A0);
  Serial.print(sensorValue);
    Serial.print(", ");
  
   //read second flex sensor
   sensorValue = analogRead(A1);
   Serial.println(sensorValue);
    delay(100);
  }
